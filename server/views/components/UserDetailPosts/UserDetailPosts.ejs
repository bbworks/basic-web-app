      <div class="user-posts">
        <h2 class="user-post-container-heading">Posts by <%- user.first_name %></h2>
        <div class="user-post-container">
          <% posts.forEach(post=>{ %>
            <%- include ("../../components/UserDetailPost/UserDetailPost", {
        			user: user,
              post: post,
        		}) %>
            <% if (posts.indexOf(post) < posts.length-1) { %>
              <hr class="post-hr" />
            <% } %>
          <% }); %>
        </div>
      </div>
